{
  "name": "ddzobov/laravel-pivot-softdeletes",
  "description": "Make your Eloquent models pivots able to soft delete in Laravel",
  "keywords": ["softdelete", "pivot", "laravel"],
  "homepage": "https://github.com/ddzobov/laravel-pivot-softdeletes",
  "type": "package",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/ddzobov/laravel-pivot-softdeletes/issues",
    "source": "https://github.com/ddzobov/laravel-pivot-softdeletes"
  },
  "authors": [
    {
      "name": "Daniil Zobov",
      "email": "ddzobov@gmail.com"
    }
  ],
  "require": {
    "laravel/framework": "^5.8|^6.0",
    "laravel/helpers": "^1.1"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "orchestra/testbench-core": "3.8.*",
    "phpunit/phpunit": "^7.5|^8.0"
  },
  "autoload": {
    "psr-4": {
      "DDZobov\\PivotSoftDeletes\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DDZobov\\PivotSoftDeletes\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}